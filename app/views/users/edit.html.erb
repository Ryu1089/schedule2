<h3>スケジュール編集</h3>

<%= form_with model:@user do |f| %>

 <table>
  <tr>
    <th><%= f.label :title,"タイトル" %></th>
    <td><%= f.text_field :title %></td>
  </tr>
  
  <tr>
    <th><%= f.label :start,"開始日" %></th>
    <td><%= f.date_field :start %></td>
  </tr>
  
  <tr>
    <th><%= f.label :end,"終了日" %></th>
    <td><%= f.date_field :end %></td>
  </tr>
  
  <tr>
    <th><%= f.label :all,"終日" %></th>
    <td><%= f.check_box :all %></td>
  </tr>
  
  <tr>
    <th><%= f.label :note,"スケジュールメモ" %></th>
    <td><%= f.text_area:note %></td>
  </tr>
 </table>
 
 <div>
   <ul>
     <li><%= f.submit"スケジュール新規登録する" %></li>
     <li><%= link_to"スケジュール一覧に戻る",:users %></li>
   </ul>
 </div>
<% end %>